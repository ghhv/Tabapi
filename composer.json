{
    "name": "mrgla55/tabapi",
    "description": "API Wrapper for the TAB",
    "license":"MIT",
    "keywords":["tab", "laravel", "rest", "api", "racing"],
    "authors": [
        {
            "name": "John Hewitt",
            "email": "john.hewitt@outlook.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mrgla55\\Tabapi\\": "src/Mrgla55/Tabapi/"
        }
    },
    "minimum-stability": "dev",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/mrgla55/Tabapi"
		}
	],
	"prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "mrgla55\\Tabapi\\ServiceProvider"
            ],
            "aliases": {
                "Tabapi": "mrgla55\\Tabapi\\Facade"
            }
        }
    },
    "require": {
        "php": ">=5.6",
        "illuminate/cache" : ">5",
        "illuminate/contracts" : ">5",
        "illuminate/config": ">5",
        "illuminate/http" : ">5",
        "illuminate/routing" : ">5",
        "guzzlehttp/guzzle": ">6.0"
    },
    "require-dev": {
        "laravel/framework": "5.5.x"
    }
}